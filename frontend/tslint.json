{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "member-access": false,
        "ordered-imports": false,
        "quotemark": false,
        "no-console": false,
        "semicolon": false,
        "jsx-no-lambda": false,
        "no-var-requires": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "node_modules/**/*.ts"
        ]
    }
}
